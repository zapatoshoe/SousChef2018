package hello;

import java.util.Arrays;
import java.util.List;

import org.springframework.stereotype.Service;

@Service
public class UserService {
	
	private List<User> users = Arrays.asList(
			new User("Ethan", "1"),
			new User("Danny", "2")
			);
			
	public List<User> getUsers() {
		return users;
	}
	
}
